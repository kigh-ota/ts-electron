{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single",
            "jsx-double"
        ],
        "object-literal-sort-keys": false,
        "interface-name": false,
        "member-ordering": false,
        "no-bitwise": false,
        "no-unused-variable": true,
        "no-empty-interface": false,
        "prettier": true
    },
    "rulesDirectory": [
        "tslint-plugin-prettier"
    ]
}